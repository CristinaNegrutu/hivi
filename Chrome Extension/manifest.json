{
 	"name": "HiVi",
  "version": "1.0",
  "description": "Offers multiple manners to view browser bookmarks and history.",
  "manifest_version": 2,
  "browser_action": {
	   "default_icon": "logo_with_no_name.png"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
      "assets/css/*",
      "assets/js/*",
      "assets/fonts/*"
  ]
}
